{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "headers": { "Content-Type": "text/css" },
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "headers": { "Content-Type": "application/javascript" },
            "dest": "/js/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}